{
  "name": "@rn-cantons/online-clicker-functions",
  "version": "0.62.2013",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "test": "FIREBASE_DATABASE_EMULATOR_HOST=9000 FIRESTORE_EMULATOR_HOST=\"localhost:8080\" jest",
    "test:watch": "FIREBASE_DATABASE_EMULATOR_HOST=9000 FIRESTORE_EMULATOR_HOST=\"localhost:8080\" jest --watch",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@rn-cantons/ts-utils": "^0.62.2013",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.16",
    "firebase-functions-test": "^0.2.0",
    "jest": "^26.6.3",
    "typescript": "^3.8.0"
  },
  "private": true
}
